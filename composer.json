{
  "name": "xakki/phpwall",
  "version": "0.5.6",
  "description": "Protect web",
  "type": "library",
  "minimum-stability": "dev",
  "license": "LGPL-2.1",
  "authors": [
    {
      "name": "Xakki",
      "email": "xakki@ya.ru"
    }
  ],
  "support": {
    "issues": "https://github.com/xakki/phpwall/issues"
  },
  "require": {
    "php": ">=5.6",
    "ext-memcached": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "1.1.9"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Xakki\\PHPWall\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs src/",
    "cs-fix": "phpcbf src/"
  }
}
